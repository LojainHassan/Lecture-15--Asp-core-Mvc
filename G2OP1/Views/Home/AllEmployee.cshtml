@model IEnumerable<Employee> 
<h1>Our Employee</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">@Html.DisplayNameFor(x=>x.Id)</th>
            <th scope="col">@Html.DisplayNameFor(x=>x.FirstName)</th>
            <th scope="col">@Html.DisplayNameFor(x=>x.LatName)</th>
            <th scope="col">@Html.DisplayNameFor(x=>x.Age)</th>
            <th scope="col">@Html.DisplayNameFor(x=>x.City)</th>
        </tr>
    </thead>
    <tbody class="table-group-divider">
        @foreach(var item in Model)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.FirstName</td>
                <td>@item.LatName</td>
                <td>@item.Age</td>
                <td>@item.City</td>
            </tr>
        }
      
       
    </tbody>
</table>
